<div class="form-group">
    <%= select "properties[#{key}]", "",
                    @character.sheet.properties[:physicals][:constants].map { |trait| [trait[:name] + '*', trait[:name]] },
                    { },
                    { multiple: true } %>
</div>

<div class="form-group row">
  <div class="col-md-6">
    <div class="array-btn btn btn-sm btn-success btn-block" data-action="add" data-properties="properties[<%=key%>]">
      <i class="fa fa-angle-down"></i>
    </div>
  </div>
  <div class="col-md-6">
    <div class="array-btn btn btn-sm btn-danger btn-block" data-action="remove" data-properties="properties[<%=key%>]">
      <i class="fa fa-angle-up"></i>
    </div>
  </div>
</div>

<div class="form-group">
    <%= select "character[character_sheet_attributes[properties[#{key}]]]", "",
                    @character.character_sheet.properties[key].map { |trait| [trait[:name] + '*', trait[:name]] },
                    { },
                    { multiple: true } %>
</div>